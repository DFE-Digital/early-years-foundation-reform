<p id="notice"><%= notice %></p>

<h1>Content Assets</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @content_assets.each do |content_asset| %>
      <tr>
        <td><%= content_asset.title %></td>
        <td><%= link_to 'Show', content_asset %></td>
        <td><%= link_to 'Edit', edit_content_asset_path(content_asset) %></td>
        <td>
        <%= button_to "delete", {:controller => :content_assets,
      :action => 'destroy', :id => content_asset.id }, :method => :delete %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Content Asset', new_content_asset_path %>
